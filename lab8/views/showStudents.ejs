<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
        <th>Delete</th>
    </tr>
    <% students.forEach((student)=> { %>
        <tr>
            <td>
                <%= student.student_id %>
            </td>
            <td>
                <%= student.student_name %>
            </td>
            <td>
                <%= student.age %>
            </td>
            <td>
                <a href="http://localhost:3000/students/delete/<%= student.student_id %>">Delete</a>
            </td>
        </tr>
        <% }) %>
</table>